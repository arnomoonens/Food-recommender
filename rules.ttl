PREFIX rule: <http://rule.stardog.com/> 
PREFIX ont: <http://www.food-groups.com/ontology#>   

[] a rule:SPARQLRule ; 
   rule:content " 
     PREFIX ont: <http://www.food-groups.com/ontology#>   
     IF { 
        ?recipe ont:requiresQuantityOfIngredient ?quantity.
        ?quantity ont:hasIngredient ?ingredient.
     } 
     THEN { 
         ?recipe ont:recipeHasIngredient ?ingredient.
     }". 